{
  "name": "react-native-keyri",
  "version": "3.0.3",
  "description": "React Native Keyri SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-keyri.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn install && yarn example && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "authentication",
    "qr-login",
    "passwordless",
    "multifactor-authentication",
    "mobile-authentication"
  ],
  "repository": "https://github.com/Keyri-Co/react-native-keyri",
  "author": "Keyri <sdk@keyri.com> (https://keyri.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Keyri-Co/react-native-keyri/issues"
  },
  "homepage": "https://github.com/Keyri-Co/react-native-keyri#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.73.2",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.47",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "react": "18.2.0",
    "react-native": "0.73.3",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "eslint-plugin-ft-flow": "^3.0.2"
  }
}
